  "version": "1.0.0",
  "description": "JavaScript client library for interacting with Toolhouse AI agents",
  "main": "index.js",
  "module": "index.js",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.js"
    }
  },
  "files": [
